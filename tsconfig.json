{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "components": [
        "./src/components"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "constants": [
        "./src/constants"
      ],
      "constants/*": [
        "./src/constants/*"
      ],
      "hooks": [
        "./src/hooks"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "navigation": [
        "./src/navigation"
      ],
      "navigation/*": [
        "./src/navigation/*"
      ],
      "screens/*": [
        "./src/screens/*"
      ],
      "screens": [
        "./src/screens"
      ],
      "assets": [
        "./src/assets"
      ],
      "assets/*": [
        "./src/assets/*"
      ],
      "store": [
        "./src/store"
      ],
      "store/*": [
        "./src/assets/*"
      ]
    }
  }
}